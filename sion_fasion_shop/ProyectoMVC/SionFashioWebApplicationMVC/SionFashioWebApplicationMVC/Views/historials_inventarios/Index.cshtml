@model IEnumerable<SionFashioWebApplicationMVC.Models.historial_inventario>

@{
    ViewData["Title"] = "Index";
}

<h1>Historial de Inventario</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.cantidad)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.tipo_cambio)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.fecha_cambio)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.id_productoNavigation)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.cantidad)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.tipo_cambio)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.fecha_cambio)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.id_productoNavigation.id_producto)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.id_historial_inventario">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.id_historial_inventario">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.id_historial_inventario">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
