@model SionFashioWebApplicationMVC.Models.registros_actividade

@{
    ViewData["Title"] = "Detalles del Registro de Actividad";
}

<h1>@ViewData["Title"]</h1>

<div>
    <h4>Detalles del Registro de Actividad</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.actividad)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.actividad)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.fecha_actividad)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.fecha_actividad)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.id_usuarioNavigation)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.id_usuarioNavigation.id_usuario)
        </dd>
    </dl>
</div>

<div>
    <a asp-action="Edit" asp-route-id="@Model?.id_registro" class="btn btn-primary">Editar</a>
    <a asp-action="Index" class="btn btn-secondary">Volver a la lista</a>
</div>
